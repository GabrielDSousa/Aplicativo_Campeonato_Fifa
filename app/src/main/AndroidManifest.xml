<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appfifa.gabriel.appfifa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bola_futebol"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bola_futebol"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label="@string/main_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FormularioCampeonatoActivity" android:label="@string/formulario_activity"/>
        <activity android:name=".AddPlayerActivity" android:label="@string/add_player_activity"/>
        <activity android:name=".AdmJogosActivity" android:label="@string/adm_jogos_activity"/>
        <activity android:name=".ResultadoActivity" android:label="@string/resultado_activity"/>
        <activity android:name=".JogoActivity" android:label="@string/jogo_activity" />
    </application>

</manifest>